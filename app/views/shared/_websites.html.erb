<ul class="result device">
<% websites.each do |website| %>
  <li>
    <h3>
      <a class="ip"><%= website.ip %></a>
    </h3>

    <hr>
    <article class="port">
      <ul class="tags">
        <li class="http_server">
        <a href=""><%= website.http_server %></a>
        </li>
        <% if website.os%>
        <li class="os">
        <a href=""><%= website.os %></a>
        </li>
      <% end %>
      </ul>
      <br/>
      <div class="timestamp">
          <i class="iconfont icon-time"></i>
          <time datetime=""><%= website.check_time%></time>
      </div>
    </article>

    <section class="banner">
        <header>
          <i><%= website.port %></i>
          <s>http</s>
        </header>
        <pre class="expand">
        <%= website.headers %>
        </pre>
    </section>
  </li>
<% end %>
</ul>
